<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="basededatos.PersonalAplicacion" table="PersonalAplicacion" lazy="false">
		<id name="idPersonal" column="IdPersonal" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="usuario" column="Usuario" type="string" length="255" not-null="false" lazy="false"/>
		<property name="contrasena" column="Contrasena" type="string" length="255" not-null="false" lazy="false"/>
		<joined-subclass name="basededatos.Administrador" table="Administrador" lazy="false">
			<key>
				<column name="PersonalAplicacionIdPersonal"/>
			</key>
			<component name="esDe" class="basededatos.BDAdministradores" update="true" insert="true">
				<component name="esDe" class="basededatos.BDPrincipal" update="true" insert="true">
					<component name="deJuegos" class="basededatos.BDJuegos" update="true" insert="true">
					</component>
					<component name="deInhabilitados" class="basededatos.BDUsuariosInhabilitados" update="true" insert="true">
					</component>
					<component name="deUserNR" class="basededatos.BDUsuariosNoRegistrados" update="true" insert="true">
					</component>
					<component name="dePersonalAtencion" class="basededatos.BDPersonalDeAtencionAlUsuario" update="true" insert="true">
					</component>
					<component name="deQuejas" class="basededatos.BDQuejasUsuarios" update="true" insert="true">
					</component>
					<component name="deChat" class="basededatos.BDChats" update="true" insert="true">
					</component>
					<component name="deUserActiv" class="basededatos.BDUsuariosActivados" update="true" insert="true">
					</component>
					<component name="deUserInac" class="basededatos.BDUsuariosInactivos" update="true" insert="true">
					</component>
					<component name="deUserExpul" class="basededatos.BDUsuariosExpulsados" update="true" insert="true">
					</component>
					<component name="dePublico" class="basededatos.BDPublicos" update="true" insert="true">
					</component>
				</component>
			</component>
		</joined-subclass>
		<joined-subclass name="basededatos.PersonaDeAtencionAlUsuario" table="PersonaDeAtencionAlUsuario" lazy="false">
			<key>
				<column name="PersonalAplicacionIdPersonal"/>
			</key>
			<set name="ORM_Redacta" lazy="true" cascade="save-update,lock" inverse="true">
				<key column="PersonaDeAtencionAlUsuarioPersonalAplicacionIdPersonal" not-null="true"/>
				<one-to-many class="basededatos.Chat"/>
			</set>
			<one-to-one name="escribe_" class="basededatos.Chat" cascade="save-update,lock" property-ref="es_utilizado_por" lazy="no-proxy" access="field"/>
			<component name="esDe" class="basededatos.BDPersonalDeAtencionAlUsuario" update="true" insert="true">
				<component name="esDe" class="basededatos.BDPrincipal" update="true" insert="true">
					<component name="deJuegos" class="basededatos.BDJuegos" update="true" insert="true">
					</component>
					<component name="deInhabilitados" class="basededatos.BDUsuariosInhabilitados" update="true" insert="true">
					</component>
					<component name="deUserNR" class="basededatos.BDUsuariosNoRegistrados" update="true" insert="true">
					</component>
					<component name="deAdmin" class="basededatos.BDAdministradores" update="true" insert="true">
					</component>
					<component name="deQuejas" class="basededatos.BDQuejasUsuarios" update="true" insert="true">
					</component>
					<component name="deChat" class="basededatos.BDChats" update="true" insert="true">
					</component>
					<component name="deUserActiv" class="basededatos.BDUsuariosActivados" update="true" insert="true">
					</component>
					<component name="deUserInac" class="basededatos.BDUsuariosInactivos" update="true" insert="true">
					</component>
					<component name="deUserExpul" class="basededatos.BDUsuariosExpulsados" update="true" insert="true">
					</component>
					<component name="dePublico" class="basededatos.BDPublicos" update="true" insert="true">
					</component>
				</component>
			</component>
		</joined-subclass>
	</class>
</hibernate-mapping>
